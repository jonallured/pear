{
  "name": "@jonallured/pear",
  "description": "Credit where credit is due.",
  "version": "0.3.0",
  "author": "Jon Allured @jonallured",
  "bin": {
    "pear": "./bin/run"
  },
  "bugs": {
    "url": "https://github.com/jonallured/pear/issues"
  },
  "dependencies": {
    "@oclif/command": "^1",
    "@oclif/config": "^1",
    "@oclif/plugin-autocomplete": "^0.1.4",
    "@oclif/plugin-help": "^3.0.0",
    "cli-ux": "^5.0.0",
    "tslib": "^1"
  },
  "devDependencies": {
    "@oclif/dev-cli": "^1",
    "@oclif/tslint": "^3",
    "@types/jest": "^25.2.1",
    "@types/node": "^13.13.4",
    "globby": "^11.0.0",
    "jest": "^25.4.0",
    "ts-jest": "^25.4.0",
    "ts-node": "^8.9.1",
    "tslint": "^6.1.2",
    "typescript": "^3.0"
  },
  "engines": {
    "node": ">=8.0.0"
  },
  "files": [
    "/bin",
    "/lib",
    "/npm-shrinkwrap.json",
    "/oclif.manifest.json"
  ],
  "homepage": "https://github.com/jonallured/pear",
  "jest": {
    "transform": {
      "^.+\\.(ts|tsx|js|jsx)$": "ts-jest"
    },
    "testRegex": ".test\\.(ts|js)",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "json"
    ]
  },
  "keywords": [
    "oclif"
  ],
  "license": "MIT",
  "main": "lib/index.js",
  "oclif": {
    "commands": "./lib/commands",
    "bin": "pear",
    "plugins": [
      "@oclif/plugin-autocomplete",
      "@oclif/plugin-help"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jonallured/pear.git"
  },
  "scripts": {
    "postpack": "rm -f oclif.manifest.json",
    "posttest": "tslint -p test -t stylish",
    "prepack": "rm -rf lib && tsc -b && oclif-dev manifest && oclif-dev readme",
    "test": "jest",
    "version": "oclif-dev readme && git add README.md"
  },
  "types": "lib/index.d.ts",
  "directories": {
    "lib": "lib",
    "test": "test"
  }
}
